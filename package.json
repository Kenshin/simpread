    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.26.1",
    "epub-press-js": "^0.5.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "minimatch": "^3.0.4",
    "nanoid": "1.0.3",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "style-loader": "^0.13.1",
